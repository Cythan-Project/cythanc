
fn sub(a, b) {
    loop {
        if b == 0 {
            return a;
        }
        dec(b);
        dec(a);
    }
}

fn add(a, b) {
    loop {
        if b == 0 {
            return a;
        }
        dec(b);
        inc(a);
    }
}

extern fn exit(exit_code);
extern fn dec(variable: Var);
extern fn inc(variable: Var);

fn main() {
    a = 10;
    inc(a);
    if add(a, 7) == 1 {
        exit(add(9,a));
    } else {
        exit(add(8,a));
    }
}